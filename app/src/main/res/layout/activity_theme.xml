<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Theme">

    <Button
        android:id="@+id/btn_Theme_one"
        android:layout_width="248dp"
        android:layout_height="126dp"
        android:text="Theme_one"
        android:background="@color/p_green"
        android:textAllCaps="false"
        android:textSize="23dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.145"
        tools:layout_editor_absoluteX="81dp"
        tools:layout_editor_absoluteY="79dp"
        tools:ignore="HardcodedText"/>

    <Button
        android:id="@+id/btn_Theme_two"
        android:layout_width="248dp"
        android:layout_height="126dp"
        android:text="Theme_two"
        android:background="@color/p_pink"

        android:textAllCaps="false"
        android:textSize="23dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.500"
        tools:layout_editor_absoluteX="81dp"
        tools:layout_editor_absoluteY="302dp"
        tools:ignore="HardcodedText"/>

    <Button
        android:id="@+id/btn_Theme_three"
        android:layout_width="248dp"
        android:layout_height="126dp"
        android:text="Theme_three"
        android:background="@color/p_blue"

        android:textAllCaps="false"
        android:textSize="23dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.855"
        tools:layout_editor_absoluteX="81dp"
        tools:layout_editor_absoluteY="515dp"
        tools:ignore="HardcodedText"/>

    <Switch
        android:id="@+id/btn_style"
        android:layout_width="81dp"
        android:layout_height="20dp"
        android:text="style"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="688dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.950"
        tools:ignore="HardcodedText"/>
</androidx.constraintlayout.widget.ConstraintLayout>